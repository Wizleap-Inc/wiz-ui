{
  "private": true,
  "name": "@wizleap-inc/wiz-ui-icons",
  "version": "0.0.0",
  "type": "module",
  "main": "index.ts",
  "scripts": {
    "build": "run-s build:icon format",
    "build:icon": "ts-node ./scripts/build.ts",
    "format": "run-p format:icon format:constants",
    "format:icon": "prettier --write \"../{wiz-ui,wiz-ui-next}/src/components/icons/*.{ts,vue}\"",
    "format:constants": "prettier --write \"../constants/component/icon-name.ts\""
  },
  "dependencies": {
    "@types/node": "^18.15.11",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2",
    "@types/rimraf": "^4.0.5",
    "rimraf": "^4.4.1"
  }
}
