{
  "name": "@wizleap-inc/wiz-ui",
  "version": "0.1.5",
  "description": "A Vue2 UI Framework for Wizleap Inc.",
  "scripts": {
    "build": "npm run build:lib && npm run build:storybook",
    "build:lib": "vue-tsc --noEmit && vite build",
    "check:lib-publish": "can-npm-publish --verbose",
    "dev": "npm run dev:storybook",
    "dev:storybook": "storybook dev -p 6006",
    "build:storybook": "storybook build",
    "test": "npm run test:unit && npm run test:storybook",
    "test:storybook": "test-storybook",
    "test:unit": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ci": "npm run test:unit && npm run test:storybook:ci",
    "test:storybook:ci": "concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue\" \"storybook build --quiet && npx http-server storybook-static --port 6006 --silent\" \"wait-on tcp:6006 && npm run test:storybook\""
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@storybook/addon-actions": "^7.0.0-alpha.48",
    "@storybook/addon-essentials": "^7.0.0-alpha.48",
    "@storybook/addon-interactions": "^7.0.0-alpha.48",
    "@storybook/addon-links": "^7.0.0-alpha.48",
    "@storybook/addons": "^6.5.13",
    "@storybook/builder-vite": "^7.0.0-alpha.48",
    "@storybook/jest": "^0.0.10",
    "@storybook/test-runner": "^0.10.0-next.1",
    "@storybook/testing-library": "^0.0.14-next.0",
    "@storybook/testing-vue": "^0.0.3",
    "@storybook/theming": "^6.5.13",
    "@storybook/vue-vite": "^7.0.0-alpha.35",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/vue": "^5.8.3",
    "@vitejs/plugin-vue2": "^1.1.2",
    "@vitest/coverage-c8": "^0.24.5",
    "babel-loader": "^8.2.5",
    "can-npm-publish": "^1.3.6",
    "concurrently": "^7.5.0",
    "http-server": "^14.1.1",
    "jsdom": "^20.0.2",
    "sass": "^1.55.0",
    "storybook": "^7.0.0-alpha.48",
    "storybook-vue-router": "^1.0.7",
    "typescript": "^4.6.4",
    "unplugin-vue-define-options": "^1.0.0",
    "vite": "^3.2.2",
    "vite-plugin-dts": "^1.4.1",
    "vitest": "^0.24.5",
    "vue-tsc": "^1.0.9",
    "wait-on": "^6.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/wizleap-inc/wiz-ui.git"
  },
  "license": "UNLICENSED",
  "author": "Wizleap Inc.",
  "files": [
    "dist"
  ],
  "main": "./dist/wiz-ui.umd.js",
  "module": "./dist/wiz-ui.es.js",
  "types": "./dist/index.d.ts",
  "dependencies": {
    "portal-vue": "^2.1.7",
    "vue": "^2.7.13",
    "vue-router": "^3.6.5"
  }
}
